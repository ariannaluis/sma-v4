{
  "name": "@sma-v4/client",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build --mode development",
    "build:emulator": "tsc && vite build --mode emulator",
    "build:live": "tsc && vite build --mode live",
    "i18n:compile": "formatjs compile temp/raw_en.json --out-file temp/en.json --format=simple",
    "i18n:extract": "formatjs extract 'src/**/*.tsx*' --ignore '**/*.d.ts' --out-file ./temp/raw_en.json --format formatter.cjs",
    "lint": "eslint",
    "preview": "vite preview",
    "serve": "vite --mode development",
    "serve:development": "vite --mode development",
    "serve:emulator": "vite --mode emulator",
    "serve:live": "vite --mode live",
    "test.e2e": "cypress run",
    "test.unit": "vitest"
  },
  "dependencies": {
    "@capacitor/app": "5.0.7",
    "@capacitor/core": "5.7.0",
    "@capacitor/haptics": "5.0.7",
    "@capacitor/ios": "^5.7.0",
    "@capacitor/keyboard": "5.0.8",
    "@capacitor/status-bar": "5.0.7",
    "@hookform/resolvers": "^3.3.4",
    "@ionic/react": "^7.0.0",
    "@ionic/react-router": "^7.0.0",
    "@material-design-icons/svg": "^0.14.13",
    "@share-meals/frg-ui": "^0.0.11",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "firebase": "^10.8.0",
    "ionicons": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-intl": "^6.6.2",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "suspend-react": "^0.1.3"
  },
  "devDependencies": {
    "@capacitor/cli": "5.7.0",
    "@formatjs/cli": "^6.2.7",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^13.5.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^0.34.6"
  },
  "description": "An Ionic project"
}
